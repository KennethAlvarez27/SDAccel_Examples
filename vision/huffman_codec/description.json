{
    "runtime": ["OpenCL"],
    "example": "Huffman Encoding/Decoding",
    "overview": [
        "This is an implementation of a huffman encoding/decoding algorithm targeting execution on an SDAccel supported FPGA acceleration card."
    ],
    "nboards": ["xilinx:adm-pcie-ku3:2ddr-xpr","xilinx:xil-accel-rd-ku115:4ddr-xpr"],
    "targets": ["sw_emu", "hw"],
    "xcl": false,
    "opencv": true,
    "cmd_args": "-p Xilinx -d ${sdx:platform} -k BUILD/default.xclbin -b PROJECT/data/rect_1024.bmp",
    "em_cmd": "./huffman -p Xilinx -d 'xilinx:adm-pcie-ku3:2ddr:3.1' --kernel ./xclbin/krnl_huffman.<emulation type>.xilinx_adm-pcie-ku3_2ddr_3_1.xclbin -b data/rect_1024.bmp",
    "hw_cmd": "../../utility/nimbix/nimbix-run.py -- ./huffman -p Xilinx -d 'xilinx:adm-pcie-ku3:2ddr:2.1' --kernel ./xclbin/krnl_huffman.hw.xilinx_adm-pcie-ku3_2ddr_3_1.xclbin data/rect_1024.bmp",
    "libs": [
        "logger", 
        "cmdparser", 
        "simplebmp", 
        "xcl"
    ], 
    "containers" : [
        {
            "name" : "krnl_huffman",
            "accelerators": [
                {
                    "name": "encode", 
                    "location": "src/krnl_huffman_singleptr.cl"
                },
                {
                    "name": "decode", 
                    "location": "src/krnl_huffman_singleptr.cl"
                }
            ]
        }
    ],
    "contributors": [
        {
            "group": "Xilinx",
            "url": "http://www.xilinx.com"
        }
    ],
    "revision": [
        {
            "date": "FEB2016",
            "version": "1.0",
            "description": "Initial Xilinx Release"
        },
        {
            "date": "JUL2016",
            "version": "2.0",
            "description": "Conversion to Makefile based compilation"
        },
        {
            "date": "DEC2016",
            "version": "3.0",
            "description": "Update for SDAccel 2016.3"
        }
    ]
}
